{
  "name": "React_Redux_Boilerplate",
  "private": true,
  "version": "0.0.2",
  "description": "Boilerplate for react redux project",
  "engines": {
    "node": ">=7.1.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "lint": "eslint ./src/",
    "start-dev": "cross-env NODE_ENV=development node server.js",
    "start": "node server.js",
    "stats": "webpack --config dist/webpack.prod.js --profile --json > stats.json",
    "build": "webpack --config dist/webpack.prod.js --production --optimize-minimize --progress --colors",
    "mocha": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require mochaconfig.js './src/**/*.spec.js' -r jsdom-global/register",
    "test": "npm run lint && npm run mocha",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "express": "4.14.0",
    "firebase": "3.6.0",
    "immutable": "3.8.1",
    "lodash.foreach": "4.5.0",
    "lodash.forin": "4.4.0",
    "material-ui": "0.17.4",
    "moment": "2.15.2",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-immutable-proptypes": "2.1.0",
    "react-input-mask": "0.7.5",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "^2.1.0",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-form": "6.1.0",
    "redux-form-material-ui": "4.1.0",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "babel-runtime": "6.23.0",
    "classnames": "2.2.5",
    "clean-webpack-plugin": "0.1.14",
    "compression-webpack-plugin": "0.3.2",
    "cross-env": "3.1.2",
    "css-loader": "0.25.0",
    "cssnano": "3.8.0",
    "enzyme": "2.5.1",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "mock-local-storage": "1.0.2",
    "node-sass": "3.10.1",
    "postcss-loader": "1.0.0",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.7.0",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.10.2",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.13.2"
  }
}
